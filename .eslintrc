{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "comma-dangle": 0,
    "no-unused-vars": "warn",
    "no-alert": "error",
    "no-unexpected-multiline": "warn"
  },
  "settings": {

  }
}